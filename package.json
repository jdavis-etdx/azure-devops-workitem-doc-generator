    "rollup-plugin-amd": "^4.0.0",
    "source-map-loader": "^3.0.1",
    "tfx-cli": "^0.11.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
