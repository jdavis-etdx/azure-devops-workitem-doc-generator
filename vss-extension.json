{
    "manifestVersion": 1,
    "id": "work-item-document-generator",
    "publisher": "JasonDavis",
    "version": "1.0.31",
    "name": "Work Item Document Generator",
    "description": "A sample Visual Studio Services extension",
    "public": false,
    "categories": ["Azure Boards"],
    "scopes":[ "vso.work" ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "panel-content",
            "type": "ms.vss-web.tab",
            "description": "The panel displaying a preview of the results",
            "targets": ["ms.vss-work-web.query-tabs"],
            "properties": {
                "uri": "index.html",
                "title": "Trace Matrix",
                "name": "Trace Matrix"
            }
        }
    ],
    "files": [
        {
            "path": "dist",
            "addressable":true
        }
    ]
}